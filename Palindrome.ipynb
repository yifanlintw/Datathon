   "source": [
    "# If a string is a palindrome\n",
    "def ispalindrome(value):\n",
    "    return value == value[::-1]\n",
    "    \n",
    "value = input(\"Please enter a string:\\n\")\n",
    "ans = ispalindrome(value)\n",
    "if ans: \n",
    "    print(\"Yes, it's a palindrome.\")\n",
    "else: \n",
    "    print(\"No, it's not a palindrome.\")"
   ]
